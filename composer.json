{
  "name": "swaggest/code-builder",
  "type": "library",
  "description": "Code generation library",
  "keywords": ["library"],
  "homepage": "https://github.com/swaggest/code-builder",
  "license": "MIT",
  "authors": [
    {
      "name": "Viacheslav Poturaev",
      "email": "vearutop@gmail.com",
      "homepage": "https://github.com/vearutop"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "psr/log": "^1.0.1",
    "php-yaoi/php-yaoi": "*"
  },
  "require-dev": {
    "phperf/phpunit": "4.8.37"
  },
  "autoload": {
    "psr-4": {
      "Swaggest\\CodeBuilder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Swaggest\\CodeBuilder\\Tests\\": "tests/src/"
    }
  },
  "scripts": {
    "test": "php -dxdebug.coverage_enable=1 ./vendor/bin/phpunit --coverage-text"
  }
}
